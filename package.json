    "ramda": "^0.21.0",
    "rethinkdb": "^2.3.2",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "date-fp": "^4.0.3",
    "axios": "^0.11.0",
    "browserify": "^13.0.1",
    "coffee-reactify": "^5.0.0",
    "coffeeify": "^2.0.1",
    "livereload": "^0.4.1",
    "parallelshell": "^2.0.0",
